<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jinlink.modules.system.mapper.SysMenuMapper">
    <select id="getAllPages" parameterType="string" resultType="string">
        SELECT sm.route_name
        FROM sys_menu sm
        WHERE sm.is_deleted = 0
    </select>
</mapper>
